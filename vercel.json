{
  "version": 2,
  "builds": [
    {
      "src": "projeto/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "projeto/api/index.py"
    }
  ],
  "crons": [
    {
      "path": "/cron/saida_automatica",
      "schedule": "0 18 * * *"
    }
  ]
}
